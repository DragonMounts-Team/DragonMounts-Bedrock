{
	"format_version": "1.10.0",
	"minecraft:client_entity": {
		"description": {
			"identifier": "dm:water_dragon",
			"materials": {
				"dragon": "dragon",
				"dragon_default": "dragon_default",
				"wing": "dragon_wings",
				"beam_material": "entity_beam",
				"death_material": "entity_beam_additive",
				"dissolve_0": "entity_dissolve_layer0.skinning",
				"dissolve_1": "entity_dissolve_layer1.skinning"
			},
			"textures": {
				"defaultdm1": "textures/entitys/water_dragon/water_dragon",
				"glow": "textures/entitys/water_dragon/glow",
				"dragon_saddled": "textures/entitys/dragon_saddled",
				"dragon_chested": "textures/entitys/dragon_chested",
				"dragon_armor_iron": "textures/entitys/dragon_armor/armor_iron",
				"dragon_armor_gold": "textures/entitys/dragon_armor/armor_gold",
				"dragon_armor_netherite": "textures/entitys/dragon_armor/armor_netherite",
				"dragon_armor_diamond": "textures/entitys/dragon_armor/armor_diamond",
				"dragon_none": "textures/entitys/none",
				"exploding": "textures/entitys/dragon_dissolve",
				"beam": "textures/entitys/endercrystal/endercrystal_beam"
			},
			"geometry": {
				"dragon_default": "geometry.mountdragon_default2dm1",							"glow": "geometry.mountdragon_default2dm1",
				"dragon_saddled": "geometry.mountdragon_saddled",
				"dragon_chested": "geometry.mountdragon_chested"
			},
			"animations": {
				"dragon_default": "animation.dragondm1.default",
				"dragon_default_b": "animation.dragondm1.default_b",
				"dragon_default_walk": "animation.dragondm1.default_walk",
				"dragon_fly": "animation.dragondm1.fly",
				"dragon_subduction": "animation.dragondm1.subduction",
				"dragon_hover": "animation.dragondm1.hover",
				"dragon_on_ground": "animation.dragondm1.on_ground",
				"dragon_sit": "animation.dragondm1.sit",
				"dragon_walk": "animation.dragondm1.walk",
				"dragon_ani": "controller.animation.dragondm1.action",
				"dragon_attack": "animation.dragondm1.attack",
				"dragon_shoot": "animation.dragondm1.shoot",
				"baby": "animation.dragondm1.baby",
				"baby1": "animation.dragondm1.baby1",
				"baby2": "animation.dragondm1.baby2",
				"baby3": "animation.dragondm1.baby3",
				"baby4": "animation.dragondm1.baby4"
			},
			"scripts": {
				"animate": [
					"dragon_default",
					"dragon_ani",
					{
						"dragon_attack": "query.is_delayed_attacking == 1"
					},
					{
					"baby": "query.variant == 0 && query.is_baby"
					},
										{
					"baby1": "query.variant == 1 && query.is_baby"
					},
										{
					"baby2": "query.variant == 2 && query.is_baby"
					},
										{
					"baby3": "query.variant == 3 && query.is_baby"
					},
										{
					"baby4": "query.variant == 4 && query.is_baby"
					}
				],
				"pre_animation": [
					"variable.death_effect_duration = 200.0;",
					"variable.death_alpha = (1.0 - (query.death_ticks + query.frame_alpha) / variable.death_effect_duration) * 2.0 + 0.3;",
					"variable.script_ride = query.has_rider;",
					"variable.bite_anim_duration = 0.75f;"
				]
			},
			"sound_effects": {
				"dragon_fly_sou": "mob.mountdragon.flap",
				"dragon_step_sou": "mob.mountdragon.step",
				"dragon_subduction": "mob.mountdragon.subduction",
				"attack_eat": "mob.mountdragon.attack.eat",
				"attack_shoot": "mob.mountdragon.shoot",
				"null": "mob.mountdragon.null"
			},
			"particle_effects": {
				"default": "dm:watering_table_particle"
			},
			"render_controllers": [
				"controller.render.dragons.is_saddled",
				"controller.render.glow",
				"controller.render.dragons.is_armored",
				"controller.render.dragons.is_chested",
				{ "controller.render.dragondm1_death_pre_pass": "query.death_ticks > 1.0" },
				{ "controller.render.dragondm1_death_main_pass": "query.death_ticks > 1.0" },
				{ "controller.render.dragondm1.default": "query.death_ticks <= 1.0" }
			],
			"spawn_egg": {
				"texture": "water_dragon_head"
			}
		}
	}
}